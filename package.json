{
  "name": "menneu",
  "private": false,
  "version": "0.5.0",
  "description": "Ménneu - Component-based extensible document processor",
  "keywords": [
    "markdown",
    "md",
    "html",
    "pdf",
    "barcode",
    "QR",
    "chart",
    "UML",
    "lisp",
    "Liyad",
    "LSX",
    "Ménneu",
    "react",
    "redagate",
    "markdown-it",
    "puppeteer"
  ],
  "main": "./bin/index.js",
  "module": "./modules/index.js",
  "modules.root": "./modules",
  "types": "./bin/index.d.ts",
  "typings": "./bin/index.d.ts",
  "engines": {
    "node": ">=10.0"
  },
  "bin": {
    "menneu": "bin.cli/menneu.js"
  },
  "dependencies": {
    "buffer": "^6.0.2",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.4.0",
    "liyad": "^0.6.0",
    "markdown-it": "^12.0.2",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-math": "4.1.1",
    "markdown-it-plantuml": "^1.4.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "paper-css": "^0.4.1",
    "puppeteer": "^5.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "red-agate": "^0.5.0",
    "red-agate-barcode": "^0.5.0",
    "red-agate-math": "^0.5.0",
    "red-agate-react-host": "^0.5.0",
    "red-agate-svg-canvas": "^0.5.0",
    "red-agate-util": "^0.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "@types/chart.js": "^2.9.28",
    "@types/highlight.js": "^9.12.4",
    "@types/jasmine": "^3.6.2",
    "@types/markdown-it": "0.0.6",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.1",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.2",
    "jasmine": "^3.6.3",
    "jasmine-spec-reporter": "^6.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.2",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "clean": "run-s clean:cjs clean:esm clean:cli clean:spec clean:dist clean:debug clean:css",
    "clean:cjs": "rimraf ./bin",
    "clean:esm": "rimraf ./modules",
    "clean:cli": "rimraf ./bin.cli",
    "clean:spec": "rimraf ./bin.test",
    "clean:dist": "rimraf ./dist",
    "clean:debug": "rimraf ./debug",
    "clean:css": "shx rm -f ./src/assets/*.min.css",
    "build": "run-s build:css build:cjs build:esm build:esm:2 build:esm:3 build:cli build:spec",
    "build:css": "run-s build:css:1 build:css:2 build:css:3 build:css:4 build:css:c:0 build:css:c:1 build:css:c:2 build:css:c:3",
    "build:css:1": "cleancss -o ./src/assets/normalize.min.css ./node_modules/normalize.css/normalize.css",
    "build:css:2": "cleancss -o ./src/assets/github-markdown.min.css ./node_modules/github-markdown-css/github-markdown.css",
    "build:css:3": "cleancss -o ./src/assets/solarized-dark.min.css ./node_modules/highlight.js/styles/solarized-dark.css",
    "build:css:4": "cleancss -o ./src/assets/dark-markdown.min.css ./src/assets/dark-markdown.css",
    "build:css:c:0": "mkdirp ./bin/assets ./modules/assets",
    "build:css:c:1": "shx cp -R ./src/assets ./bin",
    "build:css:c:2": "shx cp -R ./src/assets ./modules",
    "build:css:c:3": "shx rm -f ./bin/assets/dark-markdown.css ./modules/assets/dark-markdown.css",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir bin --declaration --declarationDir ./bin",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./modules",
    "build:esm:2": "node -e \"require('./build-scripts/copy-as-mjs').copyAsMjs('modules','modules')\"",
    "build:esm:3": "shx cp ./build-scripts/pkg.json ./modules/package.json",
    "build:spec": "webpack-cli --mode=development --config webpack.config.spec.js",
    "build:cli": "run-s build:cli:prod",
    "build:cli:prod": "cross-env NODE_ENV=production webpack-cli --mode=production --config webpack.config.js",
    "build:cli:dev": "webpack-cli --mode=development --config webpack.config.js",
    "build:dist": "run-s build:dist:prod",
    "build:dist:prod": "cross-env NODE_ENV=production webpack-cli --mode=production --config webpack.config.dist.js",
    "build:dist:dev": "webpack-cli --mode=development --config webpack.config.dist.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "prepublishOnly": "run-s clean build test lint",
    "ncu": "ncu -u -x @types/markdown-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shellyln/menneu.git"
  },
  "author": "shellyln",
  "homepage": "https://shellyln.github.io/",
  "bugs": {
    "url": "https://github.com/shellyln/menneu/issues"
  },
  "license": "ISC"
}
