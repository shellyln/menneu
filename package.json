  "scripts": {
    "clean": "run-s clean:cjs clean:esm clean:cli clean:spec clean:dist clean:debug",
    "clean:cjs": "rimraf ./bin",
    "clean:esm": "rimraf ./modules",
    "clean:cli": "rimraf ./bin.cli",
    "clean:spec": "rimraf ./bin.test",
    "clean:dist": "rimraf ./dist",
    "clean:debug": "rimraf ./debug",
    "build": "run-s build:cjs build:esm build:cli build:spec",
    "build:cjs": "tsc --module commonjs --target es2015 --outDir bin --declaration --declarationDir ./bin",
    "build:esm": "tsc --outDir modules --declaration --declarationDir ./modules",
    "build:spec": "webpack-cli --mode=development --config webpack.config.spec.js",
    "build:cli": "run-s build:cli:prod",
    "build:cli:prod": "cross-env NODE_ENV=production webpack-cli --mode=production --config webpack.config.js",
    "build:cli:dev": "webpack-cli --mode=development --config webpack.config.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "test": "jasmine",
    "prepublishOnly": "run-s clean build test"
  },
